{
  "expo": {
    "name": "GST Calculator",
    "slug": "gst-calculator",
    "description": "A simple and elegant GST calculator for Indian businesses",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.gstcalculator",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.yourcompany.gstcalculator",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A237E"
      },
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6811cd39-cf2c-4574-9785-9e27632d598f"
      }
    }
  }
}
